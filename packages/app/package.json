{
  "name": "@hedgepigdaniel/ssr-boilerplate",
  "version": "0.0.1",
  "description": "Starting point for a SSR web app",
  "repository": "https://github.com/hedgepigdaniel/ssr-boilerplate",
  "author": "Daniel Playfair Cal <daniel.playfair.cal@gmail.com>",
  "private": true,
  "scripts": {
    "babel": "babel",
    "start": "NODE_ENV=development yarn run w:babel-node server/serveDev.js",
    "start:prod": "yarn run build && yarn run serve",
    "serve": "yarn node buildServer/serveProd.js",
    "build": "yarn run build:node && yarn run build:client && yarn run build:server",
    "build:client": "w:webpack --config=buildServer/webpack.config.babel -p --progress --env.server=false",
    "build:server": "w:webpack --config=buildServer/webpack.config.babel -p --progress --env.server=true",
    "build:node": "babel --root-mode upward server/ -d buildServer/",
    "clean": "rimraf buildClient buildServer"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/component": "^5.14.1",
    "@loadable/server": "^5.14.2",
    "@loadable/webpack-plugin": "^5.14.2",
    "@respond-framework/rudy": "^0.1.1-test.9",
    "@respond-framework/types": "^0.1.1-test.4",
    "@types/loadable__webpack-plugin": "^5.7.1",
    "@types/webpack-stats-plugin": "^0.3.0",
    "@types/write-file-webpack-plugin": "^4.5.0",
    "axios": "^0.21.1",
    "core-js": "^3.9.1",
    "express": "^4.17.1",
    "nocache": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "reselect": "^4.0.0",
    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.19",
    "universal-cookie": "^4.0.4",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.1",
    "webpack-stats-plugin": "^1.0.3"
  },
  "devDependencies": {
    "extract-css-chunks-webpack-plugin": "^4.9.0"
  }
}
